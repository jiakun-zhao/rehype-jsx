{
  "name": "rehype-jsx",
  "type": "module",
  "version": "0.2.3",
  "description": "rehype plugin to Jsx",
  "author": "Jiakun Zhao <hi@zhaojiakun.com>",
  "license": "MIT",
  "homepage": "https://github.com/jiakun-zhao/rehype-jsx#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiakun-zhao/rehype-jsx.git"
  },
  "bugs": {
    "url": "https://github.com/jiakun-zhao/rehype-jsx/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && pnpm publish && npx cnpm sync rehype-jsx",
    "test": "vitest -u"
  },
  "peerDependencies": {
    "unified": "^10.1.2"
  },
  "dependencies": {
    "estree-util-to-js": "^2.0.0",
    "hast-util-to-estree": "^3.0.0"
  },
  "devDependencies": {
    "@jiakun-zhao/eslint-config": "^1.2.1",
    "@types/node": "^20.5.0",
    "bumpp": "^9.1.1",
    "eslint": "^8.47.0",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1",
    "unified": "^10.1.2"
  },
  "eslintConfig": {
    "extends": "@jiakun-zhao",
    "rules": {
      "react/jsx-indent": "off"
    }
  }
}
